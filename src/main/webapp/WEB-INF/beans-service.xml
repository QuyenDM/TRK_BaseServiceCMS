    <jaxws:endpoint id="staffService"
                    implementor="com.cms.service.StaffServiceImpl"
                    address="/StaffService">
    </jaxws:endpoint>
    <!--Chuc nang-->
    <jaxws:endpoint id="objectsService"
                    implementor="com.cms.service.ObjectsServiceImpl"
